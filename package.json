{
  "name": "node-express-api",
  "version": "1.0.0",
  "description": "API RESTful avec Express.js et SQLite",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:debug": "nodemon --inspect server.js",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --runInBand",
    "test:coverage": "jest --coverage --runInBand",
    "test:verbose": "jest --verbose --runInBand",
    "test:2fa": "jest __tests__/twoFactor.test.js",
    "test:gdpr": "jest __tests__/gdpr.test.js",
    "test:health": "jest __tests__/health.test.js",
    "test:cache": "jest __tests__/cache.test.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "docs": "node src/utils/generateDocs.js",
    "docs:serve": "npx swagger-ui-serve docs/swagger.json",
    "build": "expo export",
    "build:docker": "docker build -t mysoulmate:latest .",
    "build:docker:prod": "docker build -t mysoulmate:production --target production .",
    "backup": "node scripts/backup.js create",
    "backup:list": "node scripts/backup.js list",
    "backup:restore": "node scripts/backup.js restore",
    "backup:auto": "node scripts/autoBackup.js",
    "loadtest": "node scripts/loadtest.js",
    "loadtest:all": "node scripts/loadtest.js all",
    "cleanup": "node scripts/cleanupDatabase.js",
    "cleanup:cache": "node scripts/cleanupCache.js",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:migrate:status": "npx sequelize-cli db:migrate:status",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:reset": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate",
    "deploy:staging": "bash scripts/deploy-staging.sh",
    "deploy:production": "bash scripts/deploy-production.sh",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "health": "curl http://localhost:3000/health",
    "health:detailed": "curl http://localhost:3000/health/detailed",
    "logs:error": "tail -f logs/error.log",
    "logs:combined": "tail -f logs/combined.log",
    "validate:env": "node scripts/validateEnv.js",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "precommit": "npm run lint && npm run format:check && npm test",
    "prepare": "husky install || true"
  },
  "keywords": [
    "express",
    "api",
    "rest",
    "sqlite",
    "node"
  ],
  "author": "",
  "license": "MIT",
  "bin": {
    "mysoulmate": "./cli/mysoulmate-cli.js"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.22.2",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-community/voice": "^1.1.9",
    "@sentry/node": "^7.109.0",
    "@sentry/profiling-node": "^1.3.5",
    "autocannon": "^7.14.0",
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.0",
    "chalk": "^4.1.2",
    "commander": "^12.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "expo-auth-session": "^4.0.0",
    "expo-av": "^15.1.7",
    "expo-camera": "^14.1.0",
    "expo-file-system": "^18.1.11",
    "expo-image-picker": "^16.1.4",
    "expo-local-authentication": "^14.0.0",
    "expo-notifications": "^0.20.1",
    "expo-server-sdk": "^3.15.0",
    "expo-speech": "^13.1.7",
    "expo-web-browser": "^12.0.0",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.7.0",
    "express-status-monitor": "^1.3.4",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "hpp": "^0.2.3",
    "inquirer": "^8.2.6",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.0",
    "mixpanel": "^0.18.0",
    "ora": "^5.4.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-ical": "^0.20.1",
    "openai": "^5.8.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.3",
    "rate-limit-redis": "^4.2.0",
    "react-native-chart-kit": "^6.12.0",
    "react-native-svg": "^14.1.0",
    "sendgrid": "^5.2.3",
    "sentiment": "^5.0.2",
    "sequelize": "^6.31.1",
    "socket.io": "^4.7.4",
    "speakeasy": "^2.0.0",
    "sqlite3": "^5.1.6",
    "stripe": "^18.3.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "winston": "^3.8.2",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "eslint": "^8.41.0",
    "husky": "^9.0.11",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^3.2.5",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3"
  }
}
